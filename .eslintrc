{
  // I want to use babel-eslint for parsing!
  "parser": "babel-eslint",
  "env": {
    // I write for browser
    "browser": true,
    // in CommonJS
    "node": true,
    "jasmine": true
  },
  "extends": "eslint:recommended",
  // To give you an idea how to override rule options:
  "rules": {
    "quotes": [2, "single"],
    "angular/no-service-method": 0

  },
  "plugins": [
    "angular"
  ]
}