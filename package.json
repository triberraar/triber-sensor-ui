{
  "name": "triber-sensor-ui",
  "version": "0.0.1-SNAPSHOT",
  "description": "Angular ui for the triber-sensor backend",
  "scripts": {
    "client-dev": "webpack --config webpack.dev.js --progress -d --watch",
    "test-karma": "karma start",
    "test-karma:live": "karma start --auto-watch --no-single-run",
    "server-mock": "node server/app.js --port=3333 --mode=mock | node_modules/bunyan/bin/bunyan",
    "server-prod": "node server/app.js --port=3333 --mode=prod --proxy.host=localhost --proxy.port=8080 | node_modules/bunyan/bin/bunyan",
    "lint": "eslint public/src",
    "coveralls": "coveralls < public/build/coverage/lcov/lcov.info",

    "dev": "npm run client-dev & npm run server-mock",
    "test": "npm run lint && npm run test-karma",
    "test:live": "npm run lint & npm run test-karma:live"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/triberraar/triber-sensor-ui.git"
  },
  "author": "tribersoft",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "http-proxy": "^1.12.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "angular": "^1.4.7",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^0.11.4",
    "angular-mocks": "^1.4.7",
    "angular-ui-router": "^0.2.15",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "bunyan": "^1.5.1",
    "clean-webpack-plugin": "^0.1.5",
    "coveralls": "^2.11.4",
    "css-loader": "^0.21.0",
    "eslint": "^1.10.3",
    "eslint-config-angular": "^0.4.0",
    "eslint-plugin-angular": "^0.15.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.5.0",
    "html-webpack-plugin": "^1.6.2",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.3.4",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.21",
    "karma-webpack": "^1.7.0",
    "ng-annotate-loader": "0.0.10",
    "node-libs-browser": "^0.5.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
